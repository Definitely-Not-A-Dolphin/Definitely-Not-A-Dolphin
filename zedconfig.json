{
  "active_pane_modifiers": {
    "magnification": 1.0
  },
  "base_keymap": "VSCode",
  "buffer_font_family": "Droid Sans Mono",
  "buffer_font_size": 13,
  "buffer_font_weight": null,
  "centered_layout": {
    "left_padding": 0.2,
    "right_padding": 0.2
  },
  "chat_panel": {
    "button": "always"
  },
  "cursor_shape": "bar",
  "icon_theme": "Colored Zed Icons Theme Dark",
  "indent_guides": {
    "background_coloring": "disabled",
    "coloring": "indent_aware"
  },
  "languages": {
    "Rust": {
      "tab_size": 2
    },
    "C++": {
      "formatter": "auto"
    },
    "Svelte": {
      "formatter": "auto"
    },
    "Python": {
      "tab_size": 2,
      "formatter": null
    },
    "HTML": {
      "formatter": "language_server"
    }
  },
  "lsp": {
    "tinymist": {
      "settings": {
        "exportPdf": "onSave",
        "outputPath": "$root/$name"
      }
    },
    "wakatime": {
      "initialization_options": {
        "api-key": "-",
        "api-url": "https://hackatime.hackclub.com/api/hackatime/v1"
      }
    }
  },
  "minimap": {
    "show": "never",
    "thumb": "always"
  },
  "project_panel": {
    "auto_fold_dirs": false
  },
  "tab_size": 2,
  "theme": {
    "mode": "system",
    "light": "GitHub Dark Default",
    "dark": "GitHub Dark Default"
  },
  "ui_font_size": 16,
  "vim_mode": false
}

{
  "active_pane_modifiers": {},
  "base_keymap": "VSCode",
  "buffer_font_family": "Droid Sans Mono",
  "buffer_font_size": 13,
  "buffer_font_weight": null,
  "centered_layout": {
    "left_padding": 0.2,
    "right_padding": 0.2
  },
  "chat_panel": {
    "button": "always"
  },
  "cursor_shape": "bar",
  "format_on_save": "off",
  "icon_theme": "Colored Zed Icons Theme Dark",
  "indent_guides": {
    "background_coloring": "disabled",
    "coloring": "indent_aware"
  },
  "languages": {
    "C++": {
      "formatter": "auto"
    },
    "CSharp": {
      "tab_size": 2,
      "indent_guides": null
    },
    "HTML": {
      "formatter": "language_server"
    },
    "JavaScript": {
      "language_servers": [
        "deno",
        "!typescript-language-server",
        "!vtsls",
        "!eslint"
      ],
      "formatter": "language_server"
    },
    "Python": {
      "tab_size": 2,
      "formatter": null
    },
    "Rust": {
      "tab_size": 2
    },
    "Shell Script": {
      "format_on_save": "on",
      "formatter": {
        "external": {
          "command": "shfmt",
          "arguments": ["--filename", "{buffer_path}", "--indent", "2"]
        }
      }
    },
    "Svelte": {
      "formatter": "auto"
    },
    "TSX": {
      "language_servers": [
        "deno",
        "!typescript-language-server",
        "!vtsls",
        "!eslint"
      ],
      "formatter": "language_server"
    },
    "TypeScript": {
      "language_servers": [
        "deno",
        "!typescript-language-server",
        "!vtsls",
        "!eslint"
      ],
      "formatter": "language_server"
    }
  },
  "lsp": {
    "deno": {
      "settings": {
        "deno": {
          "enable": true
        }
      }
    },
    "omnisharp": {
      "binary": {
        "path": "/path/to/OmniSharp",
        "arguments": ["optional", "additional", "args", "-lsp"]
      }
    },
    "tinymist": {
      "settings": {
        "exportPdf": "onSave",
        "outputPath": "$root/$name"
      }
    }
  },
  "minimap": {
    "show": "never",
    "thumb": "always"
  },
  "project_panel": {
    "auto_fold_dirs": false
  },
  "tab_size": 2,
  "theme": {
    "mode": "system",
    "light": "GitHub Dark Default",
    "dark": "GitHub Dark Default"
  },
  "ui_font_size": 16,
  "vim_mode": false
}
